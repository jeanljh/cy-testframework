{
  "name": "cy-testframework",
  "version": "1.0.0",
  "description": "example of automation test framework design with page object model",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run --headed --record --key 445c0641-fdc1-4132-b674-414e94c324f1",
    "report-merge": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/result.xml",
    "report-del": "rm -rf cypress/results/junit/result.xml",
    "report-del-res": "rm -rf cypress/results/* || true",
    "del-res": "del-cli cypress/results/junit/result.xml",
    "del-res-folder": "del-cli cypress/results/junit",
    "mocha-merge": "npx mochawesome-merge cypress/results/mochawesome/*.json > mochawesome.json && npx marge mochawesome.json",
    "del-mocha-res": "del-cli cypress/results/mochawesome/*",
    "del-mocha-rep": "del-cli mochawesome-report/*",
    "test-full": "npm run del-mocha-rep && npm run del-mocha-res && npm run test && npm run mocha-merge"
  },
  "author": "jean",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^9.5.1",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-xpath": "^1.6.2",
    "del-cli": "^4.0.1",
    "dotenv": "^16.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.2",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.0.1"
  }
}
